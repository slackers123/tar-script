#def entry = main;
#use std::log;
#use std::lists::{push, get_len, get_at};

fn main() {
    int[[]] frame = [ [1, 10, 2],
                      [1, 10, 23] ];

    draw(frame);
}

fn draw(frame: int[[]]) {
    int i = 0;
    while ( i < get_len(frame)) {
        string s = "";
        int j = 0;
        while (j < get_len(get_at(frame, 0))) {
            s = s + get_at(get_at(frame, i), j);
            j = j + 1;
        }
        log(s);
        i = i + 1;
    }
}